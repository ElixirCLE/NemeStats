@model IEnumerable<UI.Models.Players.TopPlayerViewModel>

@{
    ViewBag.Title = "Top " + @Model.Count() + " Players";
    ViewBag.Description = "See who are the top Players on NemeStats!";
    Layout = MVC.Shared.Views._BaseLayout;
}

<div class="container">
    <h2 class="page-header">Top @Model.Count() Players</h2>
	@Html.Partial(MVC.Player.Views._TopPlayersPartial, @Model)
</div>